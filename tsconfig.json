  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "outDir": "dist",
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "__tests__/**/*"]
}